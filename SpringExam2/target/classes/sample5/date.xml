<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="hong" class="sample5.DateVo">
	<property name="name" value="고길동"/>
	<property name="birth" value="1990-01-01"/>
</bean>

<!-- p: 속성은 property태그를 대체한다. c:는 constructor를 대체한다. -->
<bean id="lee" class="sample5.DateVo" p:name="이순신"  p:birth="2000-12-12"/>
</beans>
<!-- 
	Spring bean configuration file : beans, p체크
 -->